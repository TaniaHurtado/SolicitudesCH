
			<div class="breadcrumb-class">Está en:<a href="index.html" target="_self" title="Inicio">Ciencias Humanas</a><span>Solicitudes > Usuarios > Inicio de Sesión</span></div>
						<h1 style="text-align: center;"> SISTEMA DE SOLICITUDES </h1>
			<h2 style="text-align: center;"> FACULTAD CIENCIAS HUMANAS </h2>
			<div class="register-content">
				<h2 class="register-label">REGISTRO</h2>

				<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
				<%= devise_error_messages! %>

				<div class="field">
					<%= f.label :nombre %><br />
					<%= f.text_field :nombre, autofocus: true %>
				</div>

				<div class="field">
					<%= f.label :apellido %><br />
					<%= f.text_field :apellido, autofocus: true %>
				</div>

				<div class="field">
					<%= f.label :teléfono %><br />
					<%= f.text_field :telefono, autofocus: true %>
				</div> 

				<div class="field">
					<%= f.label :dependencía %><br />
					<%= f.text_field :dependencia, autofocus: true %>
				</div>

				<div class="field">
					<%= f.label :cargo %><br />
					<%= f.text_field :cargo, autofocus: true %>
				</div>


			<div class="field">
				<%= f.label :correo %><br />
				<%= f.email_field :email, autofocus: true, autocomplete: "email" %>
			</div>

			<div class="field">
				<%= f.label :contraseña %>
				<% if @minimum_password_length %>
				<em>(mínimo <%= @minimum_password_length %> caracteres)</em>
				<% end %><br />
				<%= f.password_field :password, autocomplete: "off" %>
			</div>

			<div class="field">
				<%= f.label :confirmación_Contraseña %><br />
				<%= f.password_field :password_confirmation, autocomplete: "off" %>
			</div>

			<div class="actions">
				<%= f.submit "Registrar" %>
			</div>
			<% end %>
			<div class="actions">
				<%= render "devise/shared/links" %>
			</div>
		</div>
	