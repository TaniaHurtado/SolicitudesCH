<main class="detalle">
			<div class="breadcrumb-class">Est√° en:<a href="index.html" target="_self" title="Inicio">Ciencias Humanas</a><span> > Solicitudes </span></div>
						<h1 style="text-align: center;"> SISTEMA DE SOLICITUDES </h1>
			<h2 style="text-align: center;"> FACULTAD CIENCIAS HUMANAS </h2>
  
			<p id="notice"><%= notice %></p>

      <h1 style="text-align: center">Usuarios del Sistema</h1>
      <div class="links">
          <%= link_to "Salir", destroy_user_session_path , method: :delete, :class => 'navbar-link' %>|
          <%= link_to "Regresar", welcome_index_path  %> |
      </div>
      <div class="div-table">
        <table style="width:100%" class="sol_table">
          <thead>
            <tr>
              <th colspan="1">Nombre</th>
              <th colspan="1">Apellido</th>
              <th colspan="1">Correo</th>
              <th colspan="1">Dependencia</th>
              <th colspan="1">Cargo</th>
              <th colspan="1">Rol</th>
            </tr>
          </thead>

          <tbody>
            <% @users.each do |user| %>
              <tr class="sol_table">
                <td colspan="1"><%= user.nombre %></td>
                <td colspan="1"><%= user.apellido %></td>
                <td colspan="1"><%= user.email %></td>
                <td colspan="1"><%= user.dependencia %></td>
                <td colspan="1"><%= user.cargo %></td>
                <td colspan="1"><%= user.rol %></td>
                <td colspan="3" style="padding-bottom: 20px;">             
                <div class="actions boton"><%= link_to 'Editar',edit_user_path(user) %></div>
                
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <br>
</main>
 
 